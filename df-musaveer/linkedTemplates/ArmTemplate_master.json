{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-musaveer"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage_output_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_output'"
		},
		"ls_storage_account_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_storage_account_gen2'"
		},
		"AzureDataLakeStorage_output_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://samusaveeroutput.dfs.core.windows.net/"
		},
		"ls_storage_account_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://samusaveer.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-musaveer_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage_output_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_output_accountKey')]"
					},
					"ls_storage_account_gen2_accountKey": {
						"value": "[parameters('ls_storage_account_gen2_accountKey')]"
					},
					"AzureDataLakeStorage_output_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_output_properties_typeProperties_url')]"
					},
					"ls_storage_account_gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_storage_account_gen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}